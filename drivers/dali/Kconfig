menuconfig MODULE_DALI
    bool "Dali lowlevel protocol (part of IEC 60929)"
    depends on HAS_PERIPH_GPIO_IRQ
    select MODULE_PERIPH_GPIO_IRQ
    select MODULE_ZTIMER_USEC
