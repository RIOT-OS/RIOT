config MODULE_HD44780
    bool "HD44780 LCD"
    depends on HAS_PERIPH_GPIO
    depends on TEST_KCONFIG
    select MODULE_PERIPH_GPIO
    select MODULE_XTIMER
    help
        The display is also known as LCM1602C from Arduino kits.
