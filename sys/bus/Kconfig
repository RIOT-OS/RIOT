menuconfig MODULE_SYS_BUS
    bool "System buses"
    select MODULE_CORE_MSG_BUS
    help
        System buses subsystem.

config MODULE_SYS_BUS_POWER
    bool "Power bus"
    depends on MODULE_SYS_BUS
