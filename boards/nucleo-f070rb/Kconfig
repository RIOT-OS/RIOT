config BOARD
    default "nucleo-f070rb" if BOARD_NUCLEO_F070RB

config BOARD_NUCLEO_F070RB
    bool
    default y
    select BOARD_COMMON_NUCLEO64
    select CPU_MODEL_STM32F070RB

