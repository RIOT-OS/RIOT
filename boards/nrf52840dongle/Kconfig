config BOARD
    default "nrf52840dongle" if BOARD_NRF52840DONGLE

config BOARD_NRF52840DONGLE
    bool
    default y
    select BOARD_COMMON_NRF52
    select CPU_MODEL_NRF52840XXAA

source "$(RIOTBOARD)/common/nrf52/Kconfig"
