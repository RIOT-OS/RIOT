config BOARD
    default "esp32-olimex-evb" if BOARD_ESP32_OLIMEX_EVB

config BOARD_ESP32_OLIMEX_EVB
    bool
    default y
    select BOARD_COMMON_ESP32
    select CPU_MODEL_ESP32_WROOM_32

source "$(RIOTBOARD)/common/esp32/Kconfig"
