config BOARD
    default "olimexino-stm32" if BOARD_OLIMEXINO_STM32

config BOARD_OLIMEXINO_STM32
    bool
    default y
    select CPU_MODEL_STM32F103RB

    # Clock configuration
    select BOARD_HAS_HSE
    select BOARD_HAS_LSE

source "$(RIOTBOARD)/common/stm32/Kconfig"
