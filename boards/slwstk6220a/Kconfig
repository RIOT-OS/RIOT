config BOARD
    default "slwstk6220a" if BOARD_SLWSTK6220A

config BOARD_SLWSTK6220A
    bool
    default y
    select CPU_MODEL_EZR32WG330F256R60
    select HAS_PERIPH_TIMER
    select HAS_PERIPH_UART
