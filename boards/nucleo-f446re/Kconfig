config BOARD
    default "nucleo-f446re" if BOARD_NUCLEO_F446RE

config BOARD_NUCLEO_F446RE
    bool
    default y
    select BOARD_COMMON_NUCLEO64
    select CPU_MODEL_STM32F446RE

source "$(RIOTBOARD)/common/nucleo64/Kconfig"
