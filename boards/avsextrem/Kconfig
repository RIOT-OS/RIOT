config BOARD
    default "avsextrem" if BOARD_AVSEXTREM

config BOARD_AVSEXTREM
    bool
    default y
    select CPU_MODEL_LPC2387
    select HAS_PERIPH_RTC
    select HAS_PERIPH_SPI
    select HAS_PERIPH_TIMER
    select HAS_PERIPH_UART
