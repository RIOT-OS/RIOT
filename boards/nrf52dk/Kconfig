config BOARD
    default "nrf52dk" if BOARD_NRF52DK

config BOARD_NRF52DK
    bool
    default y
    select BOARDS_COMMON_NRF52XXXDK
    select CPU_MODEL_NRF52832XXAA

source "$(RIOTBOARD)/common/nrf52xxxdk/Kconfig"
