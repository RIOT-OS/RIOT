config BOARD
    default "feather-nrf52840" if BOARD_FEATHER_NRF52840

config BOARD_FEATHER_NRF52840
    bool
    default y
    select BOARD_COMMON_NRF52
    select CPU_MODEL_NRF52840XXAA

source "$(RIOTBOARD)/common/nrf52/Kconfig"
