config BOARD
    default "nucleo-g474re" if BOARD_NUCLEO_G474RE

config BOARD_NUCLEO_G474RE
    bool
    default y
    select BOARD_COMMON_NUCLEO64
    select CPU_MODEL_STM32G474RE

