config BOARD
    default "nucleo-f030r8" if BOARD_NUCLEO_F030R8

config BOARD_NUCLEO_F030R8
    bool
    default y
    select BOARD_COMMON_NUCLEO64
    select CPU_MODEL_STM32F030R8

source "$(RIOTBOARD)/common/nucleo64/Kconfig"
