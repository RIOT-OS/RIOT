config BOARD
    default "nucleo-f429zi" if BOARD_NUCLEO_F429ZI

config BOARD_NUCLEO_F429ZI
    bool
    default y
    select BOARD_COMMON_NUCLEO144
    select CPU_MODEL_STM32F429ZI

source "$(RIOTBOARD)/common/nucleo144/Kconfig"
