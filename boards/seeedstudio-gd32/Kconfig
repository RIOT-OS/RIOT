config BOARD
    default "seeedstudio-gd32" if BOARD_SEEEDSTUDIO_GD32

config BOARD_SEEEDSTUDIO_GD32
    bool
    default y
    select CPU_MODEL_GD32VF103VBT6
    select HAS_PERIPH_UART
