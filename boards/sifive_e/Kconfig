config BOARD
    default "sifive_e" if BOARD_SIFIVE_E

config BOARD_SIFIVE_E
    bool
    default y
    select CPU_MODEL_FE310_G000
    select HAS_PERIPH_TIMER
    select HAS_PERIPH_UART
