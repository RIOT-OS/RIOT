config BOARD
    default "omote" if BOARD_OMOTE

config BOARD_OMOTE
    bool
    default y
    select CPU_MODEL_CC2538SF53
    select HAS_PERIPH_ADC
    select HAS_PERIPH_I2C
    select HAS_PERIPH_RTT
    select HAS_PERIPH_SPI
    select HAS_PERIPH_TIMER
    select HAS_PERIPH_UART
