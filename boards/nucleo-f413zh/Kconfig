config BOARD
    default "nucleo-f413zh" if BOARD_NUCLEO_F413ZH

config BOARD_NUCLEO_F413ZH
    bool
    default y
    select BOARD_COMMON_NUCLEO144
    select CPU_MODEL_STM32F413ZH

source "$(RIOTBOARD)/common/nucleo144/Kconfig"
