# export the CPU name so we can differentiate between them in the code
FOO = $(shell echo $(CPU) | tr 'a-z-' 'A-Z_')
# export CFLAGS += -DCPU_$(FOO)

# include common periph module
USEMODULE += periph_common

# include stm32 common functions and stm32 common periph drivers
USEMODULE += fam_stm32 fam_stm32_periph

# export the common include directory
export INCLUDES += -I$(RIOTCPU)/fam_stm32/include

include $(RIOTMAKE)/arch/cortexm.inc.mk
